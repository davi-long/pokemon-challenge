public class Pokemon {
    private String pokemonName;
    private int powerLevel;

    private String battleType1;
    private String battleType2;

    public String getPokemonName() {
        return pokemonName;
    }
    public Pokemon(String pokemonName, int powerLevel, String battleType1, String battleType2){
        this.pokemonName = pokemonName;
        this.powerLevel = powerLevel;
        this.battleType1 = battleType1;
        this.battleType2 = battleType2;
    }
    public Pokemon(String pokemonName, int powerLevel, String battleType1){
        this.pokemonName = pokemonName;
        this.powerLevel = powerLevel;
        this.battleType1 = battleType1;
    }
    public int getPowerLevel() {
        return powerLevel;
    }

    public String getBattleType1() {
        return battleType1;
    }

    public String getBattleType2() {
        return battleType2;
    }

    String[] pokemonList = {
